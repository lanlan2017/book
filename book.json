{
    "plugins": [
        "hide-element",
        "splitter",
        "-lunr",
        "-search",
        "search-pro",
        "tbfed-pagefooter",
        "lightbox",
        "code",
        "-sharing",
        "sharing-plus",
        "intopic-toc",
        "back-to-top-button",
        "expandable-chapters-small",
        "book-summary-scroll-position-saver"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy lanlan2017.github.io 2018",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "sharing": {
            "qq": true,
            "qzone": true,
            "weibo": true,
            "all": [
                "douban", "facebook", "google", "instapaper", "linkedin", "twitter", "weibo",
                "messenger", "qq", "qzone", "viber", "whatsapp"
            ]
        },
        "intopic-toc": {
            "label": "TOC"
        }
    }
}